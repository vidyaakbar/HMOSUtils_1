    <Button
        ohos:id="$+id:button11"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:text_size="25vp"
        ohos:text_color="$ohos:color:id_color_bottom_tab_icon_auxcolor01_dark"
        ohos:background_element="#FFE30E9C"
        ohos:top_margin="12vp"
        ohos:below="$id:button5"
        ohos:right_of="$id:button10"
        ohos:left_margin="25vp"
        ohos:bottom_margin="15vp"
        ohos:right_padding="15vp"
        ohos:left_padding="15vp"
        />

    <Text
        ohos:id="$+id:colordisplayid"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:background_element="#FFF8F4F4"
        ohos:min_width="180vp"
        ohos:max_width="210vp"
        ohos:hint="Color is"
        ohos:text_size="20vp"
        ohos:below="$id:button6"
        ohos:top_margin="20vp"
        ohos:left_margin="75vp"
        ohos:left_padding="10vp"
        ohos:right_padding="10vp"
        ohos:top_padding="10vp"
        ohos:bottom_padding="10vp"
        ohos:hint_color="#FF030907"
        />
    <Text
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:id="$+id:backgrounddisplayid"
        ohos:background_element="#FFF8F4F4"
        ohos:min_width="180vp"
        ohos:max_width="210vp"
        ohos:below="$id:colordisplayid"
        ohos:hint="background is "
        ohos:text_size="20vp"
        ohos:top_margin="20vp"
        ohos:left_margin="75vp"
        ohos:left_padding="10vp"
        ohos:right_padding="10vp"
        ohos:top_padding="10vp"
        ohos:bottom_padding="10vp"
        ohos:hint_color="#FF030907"
        />

    <Button
        ohos:id="$+id:colorbutton"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:text_size="20vp"
        ohos:layout_alignment="horizontal_center"
        ohos:text="Change Color"
        ohos:below="$id:backgrounddisplayid"
        ohos:text_color="$ohos:color:id_color_bottom_tab_icon_auxcolor01_dark"
        ohos:background_element="$graphic:capsule_button_element"
        ohos:top_margin="20vp"
        ohos:left_margin="75vp"
        ohos:bottom_margin="15vp"
        ohos:right_padding="15vp"
        ohos:left_padding="15vp"
        />

    <Text
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:id="$+id:withbackgroundid"
        ohos:background_element="#FFF8F4F4"
        ohos:min_width="180vp"
        ohos:max_width="210vp"
        ohos:hint="changed color is "
        ohos:text_size="20vp"
        ohos:below="$id:colorbutton"
        ohos:top_margin="10vp"
        ohos:left_margin="75vp"
        ohos:left_padding="10vp"
        ohos:right_padding="10vp"
        ohos:top_padding="10vp"
        ohos:bottom_padding="10vp"
        ohos:hint_color="#FF030907"
        />


