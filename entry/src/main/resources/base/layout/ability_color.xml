<?xml version="1.0" encoding="utf-8"?>
<DependentLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:orientation="vertical">

    <Button
        ohos:id="$+id:button0"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:text_size="25vp"
        ohos:text_color="$ohos:color:id_color_bottom_tab_icon_auxcolor01_dark"
        ohos:background_element="#FF46EA55"
        ohos:top_margin="12vp"
        ohos:left_margin="40vp"
        ohos:bottom_margin="15vp"
        ohos:right_padding="15vp"
        ohos:left_padding="15vp"
        />

    <Button
        ohos:id="$+id:button1"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:text_size="25vp"
        ohos:text_color="$ohos:color:id_color_bottom_tab_icon_auxcolor01_dark"
        ohos:background_element="#FF52255A"
        ohos:top_margin="12vp"
        ohos:right_of="$id:button0"
        ohos:left_margin="25vp"
        ohos:bottom_margin="15vp"
        ohos:right_padding="15vp"
        ohos:left_padding="15vp"
        />


    <Button
        ohos:id="$+id:button2"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:text_size="25vp"
        ohos:text_color="$ohos:color:id_color_bottom_tab_icon_auxcolor01_dark"
        ohos:background_element="#FF29DAEA"
        ohos:top_margin="12vp"
        ohos:right_of="$id:button1"
        ohos:left_margin="25vp"
        ohos:bottom_margin="15vp"
        ohos:right_padding="15vp"
        ohos:left_padding="15vp"
        />

    <Button
        ohos:id="$+id:button3"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:text_size="25vp"
        ohos:text_color="$ohos:color:id_color_bottom_tab_icon_auxcolor01_dark"
        ohos:background_element="#EE674D4D"
        ohos:top_margin="12vp"
        ohos:right_of="$id:button2"
        ohos:left_margin="25vp"
        ohos:bottom_margin="15vp"
        ohos:right_padding="15vp"
        ohos:left_padding="15vp"
        />


    <Button
        ohos:id="$+id:button4"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:text_size="25vp"
        ohos:text_color="$ohos:color:id_color_bottom_tab_icon_auxcolor01_dark"
        ohos:background_element="#FF8A29EA"
        ohos:top_margin="12vp"
        ohos:right_of="$id:button3"
        ohos:left_margin="25vp"
        ohos:bottom_margin="15vp"
        ohos:right_padding="15vp"
        ohos:left_padding="15vp"
        />

    <Button
        ohos:id="$+id:button5"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:text_size="25vp"
        ohos:text_color="$ohos:color:id_color_bottom_tab_icon_auxcolor01_dark"
        ohos:background_element="#FFF6033B"
        ohos:top_margin="12vp"
        ohos:right_of="$id:button4"
        ohos:left_margin="25vp"
        ohos:bottom_margin="15vp"
        ohos:right_padding="15vp"
        ohos:left_padding="15vp"
        />

    <Button
        ohos:id="$+id:button6"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:text_size="25vp"
        ohos:text_color="$ohos:color:id_color_bottom_tab_icon_auxcolor01_dark"
        ohos:background_element="#FF13036E"
        ohos:top_margin="12vp"
        ohos:below="$id:button0"
        ohos:left_margin="40vp"
        ohos:bottom_margin="15vp"
        ohos:right_padding="15vp"
        ohos:left_padding="15vp"
        />
    <Button
        ohos:id="$+id:button7"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:text_size="25vp"
        ohos:text_color="$ohos:color:id_color_bottom_tab_icon_auxcolor01_dark"
        ohos:background_element="#FFDEF603"
        ohos:top_margin="12vp"
        ohos:below="$id:button1"
        ohos:right_of="$id:button6"
        ohos:left_margin="25vp"
        ohos:bottom_margin="15vp"
        ohos:right_padding="15vp"
        ohos:left_padding="15vp"
        />
    <Button
        ohos:id="$+id:button8"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:text_size="25vp"
        ohos:text_color="$ohos:color:id_color_bottom_tab_icon_auxcolor01_dark"
        ohos:background_element="#FFF5720C"
        ohos:top_margin="12vp"
        ohos:below="$id:button3"
        ohos:right_of="$id:button9"
        ohos:left_margin="25vp"
        ohos:bottom_margin="15vp"
        ohos:right_padding="15vp"
        ohos:left_padding="15vp"
        />

    <Button
        ohos:id="$+id:button9"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:text_size="25vp"
        ohos:text_color="$ohos:color:id_color_bottom_tab_icon_auxcolor01_dark"
        ohos:background_element="#FF6D4561"
        ohos:top_margin="12vp"
        ohos:below="$id:button2"
        ohos:right_of="$id:button7"
        ohos:left_margin="25vp"
        ohos:bottom_margin="15vp"
        ohos:right_padding="15vp"
        ohos:left_padding="15vp"
        />

    <Button
        ohos:id="$+id:button10"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:text_size="25vp"
        ohos:text_color="$ohos:color:id_color_bottom_tab_icon_auxcolor01_dark"
        ohos:background_element="#FF91858D"
